{
	"name": "kakoune-mode",
	"displayName": "Kakoune Mode",
	"description": "Uses Kakoune as the backend while editing",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		],
		"keybindings": [
			{
				"key": "Escape",
				"command": "extension.send_escape",
				"when": "editorTextFocus"
			},
			{
				"key": "Backspace",
				"command": "extension.send_backspace",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./ && npm run build",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"build": "fable-splitter --config splitter.config.js --commonjs",
		"fs:watch": "npm run build -- --watch",
		"lint": "tslint -p ./tsconfig.json"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.41.0",
		"fable-compiler": "^2.4.12",
		"fable-compiler-js": "^1.2.1",
		"fable-splitter": "^2.1.12",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {}
}