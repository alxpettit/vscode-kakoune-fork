{
	"name": "vscode-kakoune",
	"displayName": "Kakoune Mode",
	"description": "Kakoune editing emulation for VSCode",
	"repository": {
		"type": "git",
		"url": "https://git.sr.ht/~reykjalin/vscode-kakoune"
	},
	"version": "1.0.0",
	"publisher": "reykjalin",
	"license": "MIT",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other",
		"Keymaps"
	],
	"keywords": [
		"vim",
		"kakoune",
		"modal editing",
		"modal"
	],
	"activationEvents": [
		"*",
		"onCommand:type"
	],
	"main": "./src/Extension.bs.js",
	"contributes": {},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "npm run build",
		"watch": "npm run build -- -w",
		"pretest": "npm run compile",
		"test": "jest",
		"build": "bsb -make-world",
		"lint": "tslint -p ./tsconfig.json",
		"clean": "bsb -clean-world",
		"package": "vsce package --baseContentUrl 'https://git.sr.ht/~reykjalin/vscode-kakoune'"
	},
	"devDependencies": {
		"bs-platform": "^7.1.1",
		"@glennsl/bs-jest": "^0.5.1",
		"jest": "^26.2.2",
		"vsce": "^1.78.0"
	},
	"dependencies": {
		"tablecloth-bucklescript": "git://github.com/darklang/tablecloth#83480ad"
	}
}
